@model IEnumerable<CarrierIntegrator.preguntas_en_at>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <p>
        @Html.ActionLink("Create New", "Create")
    </p>
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.respuesta)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Preguntas.pregunta)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Usuarios.nombre_usuario)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Tsession.id_session)
            </th>
            <th></th>
        </tr>
    
    @foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.respuesta)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Preguntas.pregunta)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Usuarios.nombre_usuario)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Tsession.id_session)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id=item.id_respuesta }) |
                @Html.ActionLink("Details", "Details", new { id=item.id_respuesta }) |
                @Html.ActionLink("Delete", "Delete", new { id=item.id_respuesta })
            </td>
        </tr>
    }
    
    </table>
</body>
</html>
